        <section class="our-team pb40">
         <div class="container">
            <div class="row">
               <div class="col-md-12 col-lg-8 col-xl-9">
                  <div class="row">
                     <div class="col-lg-12">
                        <div class="courses_single_container">
                           <div class="cs_row_one">
                              <div class="cs_ins_container">
                                 <div class="cs_instructor">
                                    <ul class="cs_instrct_list float-left mb0">
                                       <li class="list-inline-item"><img class="thumb" src="/singlecourse/images/team/4.png" alt="4.png"></li>
                                       <li class="list-inline-item"><a href="#">Ali TUFAN</a></li>
                                       <li class="list-inline-item"><a href="#">Last updated 11/2019</a></li>
                                    </ul>
                                    <ul class="cs_watch_list float-right mb0">
                                       <li class="list-inline-item"><a href="#"><span class="flaticon-like"></span></a></li>
                                       <!-- <li class="list-inline-item"><a href="#">Add to Wishlist</a></li> -->
                                       <li class="list-inline-item"><a href="#"><span class="flaticon-share"> Share</span></a></li>
                                    </ul>
                                 </div>
                                 <h3 class="cs_title"><%= course.name %></h3>
                                 <ul class="cs_review_seller">
                                    <li class="list-inline-item"><a href="#"><span>Best Seller</span></a></li>
                                    <li class="list-inline-item"><a href="#"><i class="fa fa-star"></i></a></li>
                                    <li class="list-inline-item"><a href="#"><i class="fa fa-star"></i></a></li>
                                    <li class="list-inline-item"><a href="#"><i class="fa fa-star"></i></a></li>
                                    <li class="list-inline-item"><a href="#"><i class="fa fa-star"></i></a></li>
                                    <li class="list-inline-item"><a href="#"><i class="fa fa-star"></i></a></li>
                                    <li class="list-inline-item"><a href="#">4.5 (11,382 Ratings)</a></li>
                                 </ul>
                                 <ul class="cs_review_enroll">
                                    <li class="list-inline-item"><a href="#"><span class="flaticon-profile"></span> 57,869 students enrolled</a></li>
                                    <li class="list-inline-item"><a href="#"><span class="flaticon-comment"></span> 25 Review</a></li>
                                 </ul>
                              </div>
                           </div>
                           <div class="cs_row_two">
                              <div class="cs_overview">
                                 <h4 class="title">Overview</h4>
                                 <h4 class="subtitle">Course Description</h4>
                                 <p class="mb30"><%= course.description %></p>
                                 <% for(let val of course.about){ %>
                                    <p class="mb30"><%= val %></p>
                                 <% } %>
                                 <h4 class="subtitle">Duration</h4>
                                 <p class="mb30"><%= course.duration %></p>
                                 <h4 class="subtitle">Scope</h4>
                                 <ul class="cs_course_syslebus">
                                    <% for(let val of course.scope){ %>
                                    <li><i class="fa fa-check"></i><p><%= val %></p></li>
                                    <% } %>
                                 </ul>
                                 <h4 class="subtitle">Eligibility</h4>
                                 <ul class="list_requiremetn">
                                    <% for(let val of course.eligibility){ %>
                                        <li class="mb-3"><p><%= val %></p></li>
                                    <% } %>

                                    <% for(let val of course.age){ %>
                                        <li class="mb-3"><p><%= val %></p></li>
                                    <% } %>
                                 </ul>
                                 <h4 class="subtitle">Salary</h4>
                                 <p class="mb30"><%= course.salary %></p>
                              </div>
                           </div>
                     
                        </div>
                     </div>
                  </div>
               </div>
               <div class="col-lg-4 col-xl-3">
                  <div class="instructor_pricing_widget">
                     <div class="price">
                        Paid Course
                        <!-- <span>Paid</span> $49.00  -->
                        <!-- <small>$69.00</small> -->
                    </div>
                     <a href="#" class="cart_btnss">Enroll</a>
                     <a href="#" class="cart_btnss_white">Talk to an expert</a>
                     <h5 class="subtitle text-left">Includes</h5>
                     <ul class="price_quere_list text-left">
                        <li><a href="#"> 11 hours on-demand video</a></li>
                        <li><a href="#"> 69 downloadable resources</a></li>
                        <li><a href="#"> Full lifetime access</a></li>
                        <li><a href="#"> Access on mobile and TV</a></li>
                        <li><a href="#"> Assignments</a></li>
                        <li><a href="#"> Certificate of Completion</a></li>
                     </ul>
                  </div>
               </div>
            </div>
         </div>
      </section>
